{
  "root": true,
  "env": {
    "browser": true
  },
  "extends": ["airbnb-base", "plugin:react/recommended"],
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [".js",".jsx"]
      }
    }
  },
  "plugins": [
    "react"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "react/prop-types": 0,
    "react/jsx-uses-react": "error",
    "react/jsx-uses-vars": "error",
    "one-var": 0,
    "one-var-declaration-per-line": 0,
    "new-cap": 0,
    "consistent-return": 0,
    "no-param-reassign": 0,
    "no-restricted-globals": 0,
    "comma-dangle": 0,
    "curly": ["error", "multi-line"]
  }
}